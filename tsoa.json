{
  "swagger": {
    "outputDirectory": "./dist",
    "entryFile": "./src/index.ts",
    "host": "http://ios19bsh.ase.in.tum.de/dev/api",
    "basePath": "/docs",
    "specMerging": "recursive",
    "spec": {
      "paths": {
        "/campaigns/{campaignId}/images": {
          "post": {
            "consumes": ["multipart/form-data"],
            "parameters": [
              {
                "in": "formData",
                "name": "imageFile",
                "required": true,
                "type": "file"
              },
              {
                "in": "query",
                "name": "userToken",
                "required": true,
                "type": "string"
              },
              {
                "in": "path",
                "name": "campaignId",
                "required": true,
                "type": "string"
              }
            ]
          }
        },
        "/campaigns/{campaignId}/campaignImage": {
          "post": {
            "consumes": ["multipart/form-data"],
            "parameters": [
              {
                "in": "formData",
                "name": "imageFile",
                "required": true,
                "type": "file"
              },
              {
                "in": "query",
                "name": "userToken",
                "required": true,
                "type": "string"
              },
              {
                "in": "path",
                "name": "campaignId",
                "required": true,
                "type": "string"
              }
            ]
          }
        }
      }
    }
  },
  "routes": {
    "basePath": "/",
    "entryFile": "./src/index.ts",
    "routesDir": "./src"
  }
}
